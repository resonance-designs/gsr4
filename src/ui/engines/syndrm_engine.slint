    // Engine Container
    VerticalLayout {
        spacing: 8px;
        padding: 12px;

        // Engine Title & Mute
        RDSHeaderLabel {
            text: "SynDRM";
            horizontal-alignment: left;
            padding-horizontal: 12px;
            padding-vertical: 6px;
            right-padding: 12px;
            HorizontalLayout {
                spacing: 6px;
                alignment: center;
                RDSCircleToggle {
                    y: 2px;
                    active: !root.track-muted;
                    label: "Mute";
                    label-active: "Live";
                    label-pos: "left";
                    label-color: Theme.active.text_secondary;
                    label-active-color: Theme.active.text_primary;
                    label-font-size: 11px;
                    label-font-weight: 500;
                    clicked => root.toggle-track-mute();
                }
            }
        }

        // Engine Controls
        Rectangle {
            background: Theme.active.ui_device_panel_bg;
            border-width: 2px;
            border-radius: 6px;
            border-color: Theme.active.border-strong;

            // Drums
            VerticalLayout {
                HorizontalLayout {
                    spacing: 8px;
                    VerticalLayout {
                        GridLayout {
                            padding: 12px;
                            spacing-horizontal: 12px;
                            spacing-vertical: 4px;
                            // Drum Sound Controls
                            Row {
                                // Kick Drum
                                VerticalLayout {
                                    // Kick Drum Label
                                    RDSHeaderLabel {
                                        text: "Kick";
