    Rectangle {
        width: 1280px;
        background: #0f0f12;

        // Left Border
        Rectangle {
            background: #E06E26;
            width: 2px; // Thickness of the border
            x: 0; // Align to the absolute left
            height: parent.height;
        }
        // Right Border
        Rectangle {
            background: #E06E26;
            width: 2px; // Thickness of the border
            x: parent.width - self.width; // Align to the absolute right
            height: parent.height;
        }

        // Add FocusScope at the ROOT level that covers everything
        main-focus := FocusScope {
            width: 100%;
            height: 100%;
            enabled: true;

            key-pressed(event) => {
                debug("Key event:", event.text);
                if (event.text == "\u{001b}") {
                    if (root.show-settings) {
                        root.show-settings = false;
                        return accept;
                    }
                    if (root.show-bpm-keypad) {
                        root.show-bpm-keypad = false;
                        return accept;
                    }
                    if (root.show-engine-confirm) {
                        root.show-engine-confirm = false;
                        return accept;
                    }
                    if (root.show-browser) {
                        root.show-browser = false;
                        return accept;
                    }
                }
                reject
            }

            // Main UI Layout
            ScrollView {
                width: parent.width;
                height: parent.height;

                VerticalLayout {
                    width: Math.min(parent.width, 1280px);
                    x: (parent.width - self.width) / 2;
                    padding: 12px;
                    spacing: 8px;
                    // Top Bar
                    HorizontalLayout {
                        spacing: 16px;
                        // Logo / Track Select / Engine Select
                        VerticalLayout {
                            height: 84px;
                            width: 338px;
                            // MMI Logo
                            Text {
                                text: "MMI Studios";
                                font-family: "Quad Light";
                                color: #f0f0f2;
                                font-size: 44px;
                            }
                            // Instrument Name
                            Rectangle {
                                x: -10px;
                                width: 354px;
                                height: 30px;
                                background: #E06E26;
                                border-top-right-radius: 6px;
                                border-bottom-right-radius: 6px;
                                HorizontalLayout {
                                    alignment: end;
                                    padding-top: 4px;
                                    padding-bottom: 4px;
                                    padding-left: 8px;
                                    padding-right: 8px;
                                    Text {
                                        text: "TLBX-1";
                                        color: #f0f0f2;
                                        font-size: 16px;
                                        font-weight: 800;
                                        horizontal-alignment: right;
                                        vertical-alignment: center;
                                    }
                                }
                            }
                            // Track Selector
                            HorizontalLayout {
                                padding-top: 8px;
                                spacing: 8px;
                                for track[i] in [0, 1, 2, 3] : RDSSelectButton {
                                    label: "Track " + (i + 1);
                                    active: (i + 1) == root.selected-track;
                                    clicked => { root.select-track(i + 1); }
                                    shift-clicked => { root.toggle-track-mute-for(i + 1); }
                                }
                            }
                            // Engine Selector
                            HorizontalLayout {
                                width: 1176px;
                                padding-top: 12px;
                                spacing: 8px;
                                RDSComboBox {
                                    border-width: 2px;
                                    border-radius: 4px;
                                    model: root.engine-types;
                                    current-index: root.engine-index;
                                    selected => { root.engine-index = self.current-index; }
                                }
                                RDSButton { button-width: 176px; button-height: 30px; border-width: 2px; label: "Load Engine"; clicked => root.load-engine(); }
                                // Spacer
                                Rectangle {
                                    width: 2px;
                                    height: 30px;
                                    background: Theme.active.background-main;
                                }
                                // Engine Title
                                Rectangle {
                                    width: 904px;
                                    height: 30px;
                                    background: #E06E26;
                                    border-top-left-radius: 6px;
                                    border-bottom-left-radius: 6px;
                                    HorizontalLayout {
                                        alignment: start;
                                        padding-top: 4px;
                                        padding-bottom: 4px;
                                        padding-left: 8px;
                                        padding-right: 8px;
                                        Text {
                                            text: "Track " + root.selected-track + ": " +
                                                (root.active-engine-type == 1 ? "Tape-Deck"
                                                    : root.active-engine-type == 2 ? "Animate"
                                                    : root.active-engine-type == 3 ? "SynDRM"
                                                    : root.active-engine-type == 4 ? "Void Seed"
                                                    : root.active-engine-type == 5 ? "FMMI"
                                                    : root.active-engine-type == 6 ? "MonoMI"
                                                    : "None");
                                            color: #f0f0f2;
                                            font-size: 16px;
                                            font-weight: 800;
                                            horizontal-alignment: right;
                                            vertical-alignment: center;
                                        }
                                    }
                                }
                            }
                        }

                        // Clock / Transport
                        VerticalLayout {
                            spacing: 8px;
                            width: 100px;
                            height: 110px;
                            alignment: center;
                            // BPM Display
                            Text {
                                text: "BPM";
                                color: #f0f0f2;
                                font-size: 10px;
                                font-weight: 600;
                                horizontal-alignment: center;
                            }
                            RDSNumStepper {
                                x: -8px;
                                readout-width: 98px;
                                readout-height: 40px;
                                value: root.tape-tempo;
                                min-value: 20;
                                max-value: 240;
                                readout-border-width: 2px;
                                readout-border-radius: 4px;
                                button-pos: "left-right";
                                button-tog: false;
                                step: 1;
                                allow-editing: false;
                                value-changed(v) => {
                                    root.tape-tempo = v;
                                    root.tape-tempo-changed(v);
                                }
                                readout-double-clicked() => {
                                    root.bpm-keypad-input = "";
                                    root.show-bpm-keypad = true;
                                }
                            }
                            // Transport Controls
                            HorizontalLayout {
                                spacing: 8px;
                                RDSToggle {
                                    active: root.is-playing;
                                    on-icon: "stop";
                                    off-icon: "play_arrow";
                                    width: 45px;
                                    height: 45px;
                                    active-background: Theme.active.meter_normal;
                                    icon-size: 22px;
                                    border-width: 2px;
                                    border-color: #2f2f3a;
                                    clicked => root.toggle-play();
                                }
                                RDSToggle {
                                    active: root.is-recording;
                                    on-icon: "fiber_manual_record";
                                    off-icon: "album";
                                    width: 45px;
                                    height: 45px;
                                    active-background: Theme.active.meter_clip;
                                    icon-size: 22px;
                                    border-width: 2px;
                                    border-color: #2f2f3a;
                                    clicked => root.toggle-record();
                                }
                            }
                        }

                        // Master Knobs
                        HorizontalLayout {
                            spacing: 8px;
                            height: 130px;
                            // Master Gain
                            VerticalLayout {
                                spacing: 4px;
                                width: 64px;
                                alignment: center;
                                RDSKnob {
                                    renderer: "lo-fi";
                                    value: root.gain;
                                    min-value: 0;
                                    max-value: 1;
                                    label: "Master";
                                    label-pos: "top-center";
                                    size: 64px;
                                    indicator-position: 20px;
                                    sensitivity: 0.01;
                                    scroll-sensitivity: 0.01;
                                    value-changed(v) => {
