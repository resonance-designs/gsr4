                                            reflect-reverb-changed(value) => { root.reflect-reverb-changed(value); }
                                            reflect-size-changed(value) => { root.reflect-size-changed(value); }
                                            reflect-feedback-changed(value) => { root.reflect-feedback-changed(value); }
                                            reflect-spread-changed(value) => { root.reflect-spread-changed(value); }
                                            reflect-damp-changed(value) => { root.reflect-damp-changed(value); }
                                            reflect-decay-changed(value) => { root.reflect-decay-changed(value); }
                                            reflect-post-gain-changed(value) => { root.reflect-post-gain-changed(value); }
                                            void-base-freq-changed(value) => { root.void-base-freq-changed(value); }
                                            void-chaos-depth-changed(value) => { root.void-chaos-depth-changed(value); }
                                            void-entropy-changed(value) => { root.void-entropy-changed(value); }
                                            void-pan-changed(value) => { root.void-pan-changed(value); }
                                            void-width-changed(value) => { root.void-width-changed(value); }
                                            void-close-decay-changed(value) => { root.void-close-decay-changed(value); }
                                            void-filter-cutoff-changed(value) => { root.void-filter-cutoff-changed(value); }
                                            void-filter-resonance-changed(value) => { root.void-filter-resonance-changed(value); }
                                            void-drive-changed(value) => { root.void-drive-changed(value); }
                                            void-filter-pre-drive-changed(value) => { root.void-filter-pre-drive-changed(value); }
                                            void-feedback-changed(value) => { root.void-feedback-changed(value); }
                                            void-diffusion-changed(value) => { root.void-diffusion-changed(value); }
                                            void-mod-rate-changed(value) => { root.void-mod-rate-changed(value); }
                                            void-level-changed(value) => { root.void-level-changed(value); }
                                            toggle-void() => { root.toggle-void(); }
                                            toggle-mosaic-enabled => root.toggle-mosaic-enabled();
                                            toggle-mosaic-loop-lock => root.toggle-mosaic-loop-lock();
                                            mosaic-pitch-changed(value) => { root.mosaic-pitch-changed(value); }
                                            mosaic-rate-changed(value) => { root.mosaic-rate-changed(value); }
