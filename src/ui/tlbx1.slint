                        // Visualizers
                        Rectangle {
                            width: 320px;
                            height: 120px;
                            background: #121216;
                            border-radius: 6px;
                            border-color: #2a2a35;
                            border-width: 2px;
                            VerticalLayout {
                                padding: 4px;
                                spacing: 4px;
